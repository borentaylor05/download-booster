{
 "root": true,
 "env": {
  "es6": true,
  "node": true,
  "browser": true
 },
 "extends": [ "airbnb", "plugin:import/warnings" ],
 "rules": {
    "comma-dangle": 0,
    "indent": [1, 4],
    "no-plusplus": 0
 },
 "parserOptions": {
  "ecmaVersion": 6,
  "sourceType": "module",
  "ecmaFeatures": {
   "jsx": true,
   "experimentalObjectRestSpread": true
  }
 }
}
